curl -X GET --user username:password http://server/rest/

curl -X POST -H "Content-Type: multipart/form-data" --user username:password http://server/rest/ -F "file1.json=@file1.json;type=application/json"  -F "file2.txt=@file2.txt;type=text/plain"

curl -X DELETE --user username:password http://server/rest/
